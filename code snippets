#Creating a List
lista=(1,2,3,4,5,6)
print(lista)
listb=(10,20,30,40,50,60)
print(listb)
listc=(11,22,33,44,55,66)
print(listc)
#Assigning elements to different list
print(lista[3])    #Print index value3 from lista      
print(lista[-2])   #Print second last index (-2)value listb
print(listb[1:3])  #Print the value of index value 1&2 from listb
print(listc[3:])   #Print the 3 last values from the listc
print(listb[:3])   #Print the  starting value till index value 2 from listb
print(type(listc)) #Print the data type of the list











#Creating a Tupel
mylist=("C","C++","python")
mylist2=("grapes" ,"mango","oranges")
mylist3=("cadbury","nesley","amul","ferrero","saphire")
#Accessing elements from a Tupel
print(mylist[0]);     #Print the first value from mylist
print(mylist2[0:2]);  #Print the strings of the index value 0&1 from mylist2
print(len(mylist3[2]));#Print the length of the index 2 from mylist3
print(len(mylist3));   #Print the length of the mylist3






#Creating a Dictionary
dict1=dict(Bottel="Blue",Book="Yellow",Coffee="Brown")
print(dict1)
dict2=dict(Boat="Black",Cello="White",Calculator="grey",Vaseline="Yellow")
print(dict2)
dict3=dict(Rupali="Cute",Muskan="Innocent",Mukund="Jolly",Nandini="Sweet",Madhav="Arrogant")
print(dict3)
#Deleting different Dictionary elements
del(dict1["Book"])
dict2.pop("Calculator")
dict3.popitem()
print(dict1)
print(dict2)
print(dict3)



